{
  "name": "devcircles",
  "version": "1.0.0",
  "description": "Developer Circles Demo",
  "author": "Keuller Magalhaes",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --color --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "preact",
      "flow",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "chrome >= 60",
              "firefox >= 54"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "bulma": "^0.6.0",
    "history": "^4.6.3",
    "preact": "^8.2.6",
    "preact-router": "^2.5.7",
    "rxjs": "^5.5.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-preact": "^1.1.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.57.3",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "engines": {
    "node": ">= 8.6.0"
  }
}
